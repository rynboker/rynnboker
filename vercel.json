{
  "version": 2,
  "builds": [
    {
      "src": "gptlogic.py",
      "use": "@vercel/python"
    },
    {
      "src": "yt-search.py",
      "use": "@vercel/python"
    },
    {
      "src": "spotify.py",
      "use": "@vercel/python"
    },
    {
      "src": "pinterest.py",
      "use": "@vercel/python"
    },
    {
      "src": "gimage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/gptlogic",
      "dest": "/gptlogic.py"
    },
    {
      "src": "/api/ytsearch",
      "dest": "/yt-search.py"
    },
    {
      "src": "/api/spotify",
      "dest": "/spotify.py"
    },
    {
      "src": "/api/pinterest",
      "dest": "/pinterest.py"
    },
    {
      "src": "/api/gimage",
      "dest": "/gimage.py"
    }
  ]
}
