{
  "version": 2,
  "builds": [
    {
      "src": "api/gptlogic.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/yt-search.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/spotify.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/pinterest.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/gimage.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/yt-playvid.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/yt-playaud.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/gptlogic",
      "dest": "api/gptlogic.py"
    },
    {
      "src": "/api/ytsearch",
      "dest": "api/yt-search.py"
    },
    {
      "src": "/api/spotify",
      "dest": "api/spotify.py"
    },
    {
      "src": "/api/pinterest",
      "dest": "api/pinterest.py"
    },
    {
      "src": "/api/gimage",
      "dest": "api/gimage.py"
    },
    {
      "src": "/api/yt-playvid",
      "dest": "api/yt-playvid.py"
    },
    {
      "src": "/api/yt-playaud",
      "dest": "api/yt-playaud.py"
    }
  ]
}