{
  "version": 2,
  "builds": [
    {
      "src": "api/gptlogic.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/yt-search.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/spotify.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/pinterest.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/gimage.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/yt-playvid.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/yt-playaud.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/translate.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/photo2anime2.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/aboutwebsite.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/shorturl.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/gptlogic",
      "dest": "api/gptlogic.py"
    },
    {
      "src": "/api/ytsearch",
      "dest": "api/yt-search.py"
    },
    {
      "src": "/api/spotify",
      "dest": "api/spotify.py"
    },
    {
      "src": "/api/pinterest",
      "dest": "api/pinterest.py"
    },
    {
      "src": "/api/gimage",
      "dest": "api/gimage.py"
    },
    {
      "src": "/api/ytplayvid",
      "dest": "api/yt-playvid.py"
    },
    {
      "src": "/api/ytplayaud",
      "dest": "api/yt-playaud.py"
    },
    {
      "src": "/api/aboutwebsite",
      "dest": "api/aboutwebsite.py"
    },
    {
      "src": "/api/translate",
      "dest": "api/translate.py"
    },
    {
      "src": "/api/photo2anime2",
      "dest": "api/photo2anime2.py"
    },
    {
      "src": "/api/shorturl",
      "dest": "api/shorturl.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/shorturl.py"
    },
    {
      "src": "/tmp/(.*)",
      "dest": "tmp/temp_images/$1"
    }
  ],
  "env": {
    "FLASK_DEBUG": "true",
    "FLASK_ENV": "production"
  }
}
