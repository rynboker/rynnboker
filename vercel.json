{
  "version": 2,
  "builds": [
    {
      "src": "api/gptlogic.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/yt-search.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/gptlogic",
      "dest": "api/gptlogic.py"
    },
    {
      "src": "/api/ytsearch",
      "dest": "api/yt-search.py"
    }
  ]
}
